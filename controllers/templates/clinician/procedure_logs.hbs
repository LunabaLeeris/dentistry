<h1 class="display-4 mt-3"><strong>Procedure Logs</strong></h1>
<p3 class="display-7">Last Version Saved: {{LAST-DATE}} </p3>
<div class="col align-items-center mt-5">
    <!-- SEARCH BAR -->
    <div class="row d-flex justify-content-between no-gutters align-items-center">
        <div class="col">
            <form class="d-flex align-items-center">
                <input class="form-control me-3" type="search" placeholder="Search" aria-label="Search">
                <button class="btn ms-0 rounded-0 border" type="submit">
                    <img src="\assets/icons/search.svg" alt="">
                </button>
            </form>
        </div>
        <!-- FILTER -->
        <div class="col-auto me-2 dropdown">
            <button class="btn btn-secondary dropdown-toggle mt-0" type="button" id="filter-lab-logs" data-bs-toggle="dropdown" aria-expanded="false">
                    <img src="\assets/icons/filter.svg">
            </button>
            <ul class="dropdown-menu" aria-labelledby="dropdownMenuButton">
                <li><a class="dropdown-item" href="#" onclick="sort(most_recent)">Most recent</a></li>
                <li><a class="dropdown-item" href="#" onclick="sort(oldest)">Oldest</a></li>
                <li><a class="dropdown-item" href="#" onclick="sort(instructor)">Instructor</a></li>
            </ul>
        </div>
        <!-- ADD NEW -->
        <div class="col-auto">
            <button id="add-new-proced-log" type="button" class="btn btn-success rounded-0">
                Add new procedure log
            </button>
        </div>
    </div>
</div>
<!-- TABLE DATA -->
<div class="col table-responsive mt-1">
    <table class="table table-md table-bordered mt-3">
        <thead>
            <tr class="table-primary">
            <th scope="col">#</th> 
            <th scope="col">ReportID</th>
            <th scop e="col">Procedure Date</th>
            <th scop e="col">Clinic Level</th>
            <th scope="col">Room Number</th>
            <th scope="col">Chair #</th>
            <th scope="col">Procedure Section</th>
            <th scope="col">Procedure Type</th>
            <th scope="col">Instructor Name</th>
            <th scope="col">Instructor ID</th>
            <th scope="col">Time Start</th>
            <th scope="col">Time Completion</th>
            <th scope="col">Patient Name</th>
            <th scope="col">Grade</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                {{TABLE-DATA}}
            </tr>
        </tbody>
    </table>
</div>
{{EMPTY-TAG}}